@HAVE_GTK_DOC_TRUE@all-local: html-build.stamp

#### scan ####

# in the case of non-srcdir builds, the built gst directory gets added
# to gtk-doc scanning; but only then, to avoid duplicates
@HAVE_GTK_DOC_TRUE@scan-build.stamp: $(HFILE_GLOB) $(SCANOBJ_DEPS) $(basefiles)
@HAVE_GTK_DOC_TRUE@	@echo '*** Scanning header files ***'
@HAVE_GTK_DOC_TRUE@	if grep -l '^..*$$' $(srcdir)/$(DOC_MODULE).types > /dev/null;	\
@HAVE_GTK_DOC_TRUE@	then								\
@HAVE_GTK_DOC_TRUE@	    if test x"$(srcdir)" != x. ; then				\
@HAVE_GTK_DOC_TRUE@	        cp $(srcdir)/$(DOC_MODULE).types . ;			\
@HAVE_GTK_DOC_TRUE@	        chmod u+w $(DOC_MODULE).types ;				\
@HAVE_GTK_DOC_TRUE@	    fi ;							\
@HAVE_GTK_DOC_TRUE@	    CC="$(GTKDOC_CC)" LD="$(GTKDOC_LD)" 			\
@HAVE_GTK_DOC_TRUE@	    CFLAGS="$(GTKDOC_CFLAGS)" LDFLAGS="$(GTKDOC_LIBS)"		\
@HAVE_GTK_DOC_TRUE@	    $(GTK_DOC_SCANOBJ) --type-init-func="gnl_init(NULL,NULL)"	\
@HAVE_GTK_DOC_TRUE@	        --module=$(DOC_MODULE) ;				\
@HAVE_GTK_DOC_TRUE@	else								\
@HAVE_GTK_DOC_TRUE@	    cd $(srcdir) ;						\
@HAVE_GTK_DOC_TRUE@	    for i in $(SCANOBJ_FILES) ; do				\
@HAVE_GTK_DOC_TRUE@               test -f $$i || touch $$i ;				\
@HAVE_GTK_DOC_TRUE@	    done							\
@HAVE_GTK_DOC_TRUE@	fi
@HAVE_GTK_DOC_TRUE@	if test "x$(top_srcdir)" != "x$(top_builddir)";			\
@HAVE_GTK_DOC_TRUE@        then								\
@HAVE_GTK_DOC_TRUE@          export BUILT_OPTIONS="--source-dir=$(top_builddir)/gnl";	\
@HAVE_GTK_DOC_TRUE@        fi;								\
@HAVE_GTK_DOC_TRUE@	gtkdoc-scan							\
@HAVE_GTK_DOC_TRUE@		$(SCAN_OPTIONS) $(EXTRA_HFILES)				\
@HAVE_GTK_DOC_TRUE@		--module=$(DOC_MODULE)					\
@HAVE_GTK_DOC_TRUE@		--source-dir=$(DOC_SOURCE_DIR)				\
@HAVE_GTK_DOC_TRUE@		$$BUILT_OPTIONS						\
@HAVE_GTK_DOC_TRUE@		--ignore-headers="$(IGNORE_HFILES)"
@HAVE_GTK_DOC_TRUE@	touch scan-build.stamp

@HAVE_GTK_DOC_TRUE@$(DOC_MODULE)-decl.txt $(SCANOBJ_FILES): scan-build.stamp
@HAVE_GTK_DOC_TRUE@	@true

#### templates ####

@HAVE_GTK_DOC_TRUE@tmpl-build.stamp: $(DOC_MODULE)-decl.txt $(SCANOBJ_FILES) $(DOC_MODULE)-sections.txt $(DOC_OVERRIDES)
@HAVE_GTK_DOC_TRUE@	@echo '*** Rebuilding template files ***'
@HAVE_GTK_DOC_TRUE@	if test x"$(srcdir)" != x. ; then \
@HAVE_GTK_DOC_TRUE@	    cp $(srcdir)/$(DOC_MODULE)-sections.txt . ; \
@HAVE_GTK_DOC_TRUE@	    touch $(DOC_MODULE)-decl.txt ; \
@HAVE_GTK_DOC_TRUE@	fi
@HAVE_GTK_DOC_TRUE@	gtkdoc-mktmpl --module=$(DOC_MODULE) | tee tmpl-build.log
@HAVE_GTK_DOC_TRUE@	@if grep "WARNING:" tmpl-build.log > /dev/null; then \
@HAVE_GTK_DOC_TRUE@	    cat $(DOC_MODULE)-unused.txt ; \
@HAVE_GTK_DOC_TRUE@	    exit 1; fi
@HAVE_GTK_DOC_TRUE@	rm -f tmpl-build.log
@HAVE_GTK_DOC_TRUE@	touch tmpl-build.stamp

@HAVE_GTK_DOC_TRUE@tmpl.stamp: tmpl-build.stamp
@HAVE_GTK_DOC_TRUE@	@true

#### xml ####

@HAVE_GTK_DOC_TRUE@sgml-build.stamp: tmpl.stamp $(CFILE_GLOB)
@HAVE_GTK_DOC_TRUE@	@echo '*** Building XML ***'
@HAVE_GTK_DOC_TRUE@	gtkdoc-mkdb --module=$(DOC_MODULE) --source-dir=$(DOC_SOURCE_DIR) --main-sgml-file=$(srcdir)/$(DOC_MAIN_SGML_FILE) --output-format=xml $(MKDB_OPTIONS) | tee sgml-build.log
@HAVE_GTK_DOC_TRUE@	@if grep "WARNING:" sgml-build.log > /dev/null; then exit 1; fi
@HAVE_GTK_DOC_TRUE@	rm sgml-build.log
@HAVE_GTK_DOC_TRUE@	touch sgml-build.stamp

@HAVE_GTK_DOC_TRUE@sgml.stamp: sgml-build.stamp
@HAVE_GTK_DOC_TRUE@	@true

#### html ####

# FIXME: setting LANG=C is a hack to work with gtk-doc < 1.1, because
# that forces a non-utf8 locale.  gtk-doc >= 1.1 solves this by calling
# use bytes; in gtkdoc-fixxref
@HAVE_GTK_DOC_TRUE@html-build.stamp: sgml.stamp $(DOC_MAIN_SGML_FILE) $(content_files)
@HAVE_GTK_DOC_TRUE@	@echo '*** Building HTML ***'
@HAVE_GTK_DOC_TRUE@	if test -d html; then rm -rf html; fi
@HAVE_GTK_DOC_TRUE@	mkdir html
@HAVE_GTK_DOC_TRUE@	cp $(srcdir)/$(DOC_MAIN_SGML_FILE) html
@HAVE_GTK_DOC_TRUE@	cp -pr xml html
#cp ../version.entities html
@HAVE_GTK_DOC_TRUE@	cd html && gtkdoc-mkhtml $(DOC_MODULE) $(DOC_MAIN_SGML_FILE)
@HAVE_GTK_DOC_TRUE@	rm -f html/$(DOC_MAIN_SGML_FILE)
@HAVE_GTK_DOC_TRUE@	rm -rf html/xml
#rm -f html/version.entities
@HAVE_GTK_DOC_TRUE@	test "x$(HTML_IMAGES)" = "x" || for i in "" $(HTML_IMAGES) ; do \
@HAVE_GTK_DOC_TRUE@	    if test "$$i" != ""; then cp $(srcdir)/$$i html ; fi; done
@HAVE_GTK_DOC_TRUE@	@echo '-- Fixing Crossreferences' 
@HAVE_GTK_DOC_TRUE@	LANG=C && gtkdoc-fixxref --module-dir=html --html-dir=$(HTML_DIR) $(FIXXREF_OPTIONS)
@HAVE_GTK_DOC_TRUE@	touch html-build.stamp
@HAVE_GTK_DOC_FALSE@all-local:

clean-local:
	rm -f *~ *.bak
	rm -rf xml html
	rm -rf .libs

maintainer-clean-local: clean
	cd $(srcdir) && rm -rf xml html $(DOC_MODULE)-decl-list.txt $(DOC_MODULE)-decl.txt

# company: don't delete .sgml and -sections.txt as they're in CVS
